  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && cross-env STAGE_ENV=lemon yarn webpack --config webpack.config.api.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:prod": "node dist/main",
    "start-api1": "cross-env STAGE_ENV=remote-prod DEBUG_MODE=debug node ./build/out.js",
    "start-api2": "cross-env STAGE_ENV=korea DEBUG_MODE=debug ts-node -r tsconfig-paths/register src/fake-api/main.ts",
    "start-api:prod": "cross-env STAGE_ENV=remote-prod FAKE_VENDOR=fchev DEBUG_MODE=debug node ./dist/fake-evo/src/fake-api/main.js",
    "start-api:tomato": "cross-env STAGE_ENV=tomato FAKE_VENDOR=fcevo DEBUG_MODE=debug node ./dist/fake-evo/src/fake-api/main.js",
    "start-api:prod-local": "cross-env STAGE_ENV=prod-local node ./dist/fake-evo/src/fake-api/main.js",
    "start-ws:prod": "node ./dist/fake-evo/src/tools/websocket-test.js",
    "build:resolver": "tsc && cross-env STAGE_ENV=prod pnpm webpack --entry ./dist/fake-evo/src/fake-resolver/main.js",
    "build:history": "tsc && cross-env STAGE_ENV=prod pnpm webpack --entry ./dist/fake-evo/src/fake-history/node.js",
    "build:api1": "esbuild src/fake-api/main.ts --bundle --sourcemap=inline --platform=node --outfile=esbuild/out.js",
    "build:api2": "tsc && cross-env STAGE_ENV=prod pnpm webpack --entry ./dist/src/fake-api/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
